{
    "presets": [
        "minify",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-function-name"],
        ["@babel/plugin-transform-parameters"],
        ["@babel/plugin-transform-template-literals"],
        ["@babel/plugin-transform-arrow-functions"]
    ],
    "sourceMaps": true
  }